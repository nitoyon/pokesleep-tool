{
  "name": "research-calc",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "homepage": "https://nitoyon.github.io/pokesleep-tool/",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "@use-gesture/react": "^10.3.1",
    "i18next": "^25.5.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-i18next": "^16.0.0",
    "react-spring": "^10.0.3"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "typecheck": "tsc --noEmit && tsc --noEmit -p tsconfig.script.json",
    "lint": "eslint src",
    "verify": "npm run typecheck && npm run lint && npm run test -- run && npm run build",
    "preview": "vite preview",
    "rpfit": "npx ts-node --project tsconfig.script.json scripts/rpfit.ts",
    "rptest": "npx ts-node --project tsconfig.script.json scripts/rptest.ts",
    "skillvaluefit": "npx ts-node --project tsconfig.script.json scripts/skillvaluefit.ts",
    "test": "vitest"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.5.0",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.0.2",
    "baseline-browser-mapping": "^2.9.14",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "jsdom": "^27.0.0",
    "typescript-eslint": "^8.44.1",
    "vite": ">=7.1.11",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "js-yaml": ">=4.1.1",
    "react-spring": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  }
}
