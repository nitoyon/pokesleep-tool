{
      "compilerOptions": {
        "target": "es2022",
        "lib": [
          "dom",
          "dom.iterable",
          "esnext"
        ],
        "allowJs": true,
        "noImplicitAny": true,
        "skipLibCheck": false,
        "downlevelIteration": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "module": "CommonJS",
        "moduleResolution": "node",
        "isolatedModules": false,
        "resolveJsonModule": true,
        "noEmit": true,
        "jsx": "preserve"
      },
      "include": [
        "scripts"
      ]
}
